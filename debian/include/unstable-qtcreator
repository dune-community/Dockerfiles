RUN cd /tmp && \
    wget http://ftp.fau.de/qtproject/archive/qtcreator/4.2/4.2.2/qt-creator-opensource-linux-x86_64-4.2.2.run && \
    wget https://raw.githubusercontent.com/benlau/qtci/master/bin/extract-qt-installer && \
    chmod 755 extract-qt-installer && \
    ./extract-qt-installer qt-creator-opensource-linux-x86_64-4.2.2.run /usr/local/ && \
    rm extract-qt-installer qt-creator-opensource-linux-x86_64-4.2.2.run
